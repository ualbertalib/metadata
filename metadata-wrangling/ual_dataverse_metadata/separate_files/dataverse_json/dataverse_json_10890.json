"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"castroco@ualberta.ca"}}]},{"typeName":"dsDescription","multiple":true,"typeClass":"compound","value":[{"dsDescriptionValue":{"typeName":"dsDescriptionValue","multiple":false,"typeClass":"primitive","value":"The purpose of this scoping review is to provide an overview of the evidence for the use of long-term non-invasive ventilation (NIV) in children."}}]},{"typeName":"subject","multiple":true,"typeClass":"controlledVocabulary","value":["Medicine, Health and Life Sciences"]},{"typeName":"keyword","multiple":true,"typeClass":"compound","value":[{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"ventilation therapies"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"pediatrics"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"MeSH"},"keywordVocabularyURI":{"typeName":"keywordVocabularyURI","multiple":false,"typeClass":"primitive","value":"https://www.nlm.nih.gov/mesh/"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"respiratory insufficiencies"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"positive airway pressure"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"noninvasive ventilation"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"MeSH"},"keywordVocabularyURI":{"typeName":"keywordVocabularyURI","multiple":false,"typeClass":"primitive","value":"https://www.nlm.nih.gov/mesh/"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"review"},"keywordVocabulary":{"typeName":"keywordVocabulary","multiple":false,"typeClass":"primitive","value":"MeSH"},"keywordVocabularyURI":{"typeName":"keyword