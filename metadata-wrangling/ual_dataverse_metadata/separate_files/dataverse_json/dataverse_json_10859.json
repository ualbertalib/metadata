{"id":735,"identifier":"DVN/10859","persistentUrl":"https://doi.org/10.7939/DVN/10859","protocol":"doi","authority":"10.7939","publisher":"UAL Dataverse","publicationDate":"2016-08-25","storageIdentifier":"file://10.7939/DVN/10859","datasetVersion":{"id":950,"datasetId":735,"datasetPersistentId":"doi:10.7939/DVN/10859","storageIdentifier":"file://10.7939/DVN/10859","versionNumber":1,"versionMinorNumber":0,"versionState":"RELEASED","versionNote":"","distributionDate":"2016-08","productionDate":"2016-08","lastUpdateTime":"2016-08-25T19:45:37Z","releaseTime":"2016-08-25T00:00:00Z","createTime":"2016-08-25T19:44:35Z","license":"NONE","fileAccessRequest":false,"metadataBlocks":{"citation":{"displayName":"Citation Metadata","fields":[{"typeName":"title","multiple":false,"typeClass":"primitive","value":"Just-in-Time Static Analysis"},{"typeName":"author","multiple":true,"typeClass":"compound","value":[{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Nguyen, Lisa"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Fraunhofer IEM"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Ali, Karim"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"University of Alberta"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Livshits, Ben"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Microsoft Research"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Bodden, Eric"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"Paderborn University and Fraunhofer IEM"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Smith, Justin"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"North Carolina State University"}},{"authorName":{"typeName":"authorName","multiple":false,"typeClass":"primitive","value":"Murphy-Hill, Emerson"},"authorAffiliation":{"typeName":"authorAffiliation","multiple":false,"typeClass":"primitive","value":"North Carolina State University"}}]},{"typeName":"datasetContact","multiple":true,"typeClass":"compound","value":[{"datasetContactName":{"typeName":"datasetContactName","multiple":false,"typeClass":"primitive","value":"Lisa Nguyen"},"datasetContactAffiliation":{"typeName":"datasetContactAffiliation","multiple":false,"typeClass":"primitive","value":"Fraunhofer IEM"},"datasetContactEmail":{"typeName":"datasetContactEmail","multiple":false,"typeClass":"primitive","value":"lisa.nguyen@iem.fraunhofer.de"}}]},{"typeName":"dsDescription","multiple":true,"typeClass":"compound","value":[{"dsDescriptionValue":{"typeName":"dsDescriptionValue","multiple":false,"typeClass":"primitive","value":"We present the concept of Just-In-Time (JIT) static analysis that interleaves code development and bug fixing in an integrated development environment. Unlike traditional static analysis tools, a JIT analysis tool presents warnings to code developers over time, providing the most relevant results quickly, and computing less relevant results incrementally later. We describe general guidelines for designing JIT analyses. We also present a general recipe for turning static data- flow analyses into JIT analyses through a concept of layered analysis execution illustrated through Cheetah, a JIT taint analysis for Android applications. Our empirical evaluation of Cheetah on real-world applications shows that our approach returns warnings quickly enough to avoid disrupting a developerâ€™s workflow, a finding confirmed by developers in our user study."},"dsDescriptionDate":{"typeName":"dsDescriptionDate","multiple":false,"typeClass":"primitive","value":"2016-08"}}]},{"typeName":"keyword","multiple":true,"typeClass":"compound","value":[{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"static analysis"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"just-in-time"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"taint analysis"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"android"}},{"keywordValue":{"typeName":"keywordValue","multiple":false,"typeClass":"primitive","value":"analysis tools"}}]},{"typeName":"producer","multiple":true,"typeClass":"compound","value":[{"producerName":{"typeName":"producerName","multiple":false,"typeClass":"primitive","value":"Lisa Nguyen"},"producerAffiliation":{"typeName":"producerAffiliation","multiple":false,"typeClass":"primitive","value":"Fraunhofer IEM"}}]},{"typeName":"productionDate","multiple":false,"typeClass":"primitive","value":"2016-08"},{"typeName":"distributor","multiple":true,"typeClass":"compound","value":[{"distributorName":{"typeName":"distributorName","multiple":false,"typeClass":"primitive","value":"Lisa Nguyen"},"distributorAffiliation":{"typeName":"distributorAffiliation","multiple":false,"typeClass":"primitive","value":"Fraunhofer IEM"}}]},{"typeName":"distributionDate","multiple":false,"typeClass":"primitive","value":"2016-08"},{"typeName":"dateOfDeposit","multiple":false,"typeClass":"primitive","value":"2016-08-25"}]}},"files":[{"description":"Just-in-Time Static Analysis","label":"jit.pdf","restricted":false,"version":1,"datasetVersionId":950,"categories":["Technical Report"],"dataFile":{"id":6728,"persistentId":"","pidURL":"","filename":"jit.pdf","contentType":"application/pdf","filesize":584309,"description":"Just-in-Time Static Analysis","storageIdentifier":"file://21514","rootDataFileId":-1,"md5":"ba1284f0768d965c481bbe95024859f4","checksum":{"type":"MD5","value":"ba1284f0768d965c481bbe95024859f4"},"creationDate":"2016-08-25"}}],"c