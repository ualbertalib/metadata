# dataverse_export_sep
# XML formats:  dcterms ddi Datacite oai_ddi oai_dc oai_datacite
# JSON formats: dataverse_json OAI_ORE schema.org


while read -r line; do
    curl "https://dataverse.library.ualberta.ca/api/datasets/export?exporter=schema.org&persistentId=doi:10.7939/DVN/$line" > schema_org_$line.json
    echo "Text read from file: $line"
done < ual_dataverse_DOIs_slim.txt