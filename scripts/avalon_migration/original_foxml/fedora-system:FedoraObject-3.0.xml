<foxml:datastreamVersion ID="POLICY-SCHEMA1.0" LABEL="POLICY xml schema" CREATED="2016-08-12T20:16:12.115Z" MIMETYPE="text/xml" SIZE="15983">
<foxml:xmlContent>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xacml="urn:oasis:names:tc:xacml:1.0:policy" targetNamespace="urn:oasis:names:tc:xacml:1.0:policy" elementFormDefault="qualified" attributeFormDefault="unqualified">
                    
                    <xs:element name="PolicySet" type="xacml:PolicySetType"></xs:element>
                    <xs:complexType name="PolicySetType">
                        <xs:sequence>
                            <xs:element ref="xacml:Description" minOccurs="0"></xs:element>
                            <xs:element ref="xacml:PolicySetDefaults" minOccurs="0"></xs:element>
                            <xs:element ref="xacml:Target"></xs:element>
                            <xs:choice minOccurs="0" maxOccurs="unbounded">
                                <xs:element ref="xacml:PolicySet"></xs:element>
                                <xs:element ref="xacml:Policy"></xs:element>
                                <xs:element ref="xacml:PolicySetIdReference"></xs:element>
                                <xs:element ref="xacml:PolicyIdReference"></xs:element>
                            </xs:choice>
                            <xs:element ref="xacml:Obligations" minOccurs="0"></xs:element>
                        </xs:sequence>
                        <xs:attribute name="PolicySetId" type="xs:anyURI" use="required"></xs:attribute>
                        <xs:attribute name="PolicyCombiningAlgId" type="xs:anyURI" use="required"></xs:attribute>
                    </xs:complexType>
                    
                    <xs:element name="PolicySetIdReference" type="xs:anyURI"></xs:element>
                    <xs:element name="PolicyIdReference" type="xs:anyURI"></xs:element>
                    
                    <xs:element name="PolicySetDefaults" type="xacml:DefaultsType"></xs:element>
                    <xs:element name="PolicyDefaults" type="xacml:DefaultsType"></xs:element>
                    <xs:complexType name="DefaultsType">
                        <xs:sequence>
                            <xs:choice>
                                <xs:element ref="xacml:XPathVersion"></xs:element>
                            </xs:choice>
                        </xs:sequence>
                    </xs:complexType>
                    
                    <xs:element name="XPathVersion" type="xs:anyURI"></xs:element>
                    
                    <xs:element name="Policy" type="xacml:PolicyType"></xs:element>
                    <xs:complexType name="PolicyType">
                        <xs:sequence>
                            <xs:element ref="xacml:Description" minOccurs="0"></xs:element>
                            <xs:element ref="xacml:PolicyDefaults" minOccurs="0"></xs:element>
                            <xs:element ref="xacml:Target"></xs:element>
                            <xs:element ref="xacml:Rule" minOccurs="0" maxOccurs="unbounded"></xs:element>
                            <xs:element ref="xacml:Obligations" minOccurs="0"></xs:element>
                        </xs:sequence>
                        <xs:attribute name="PolicyId" type="xs:anyURI" use="required"></xs:attribute>
                        <xs:attribute name="RuleCombiningAlgId" type="xs:anyURI" use="required"></xs:attribute>
                    </xs:complexType>
                    
                    <xs:element name="Description" type="xs:string"></xs:element>
                    
                    <xs:element name="Rule" type="xacml:RuleType"></xs:element>
                    <xs:complexType name="RuleType">
                        <xs:sequence>
                            <xs:element ref="xacml:Description" minOccurs="0"></xs:element>
                            <xs:element ref="xacml:Target" minOccurs="0"></xs:element>
                            <xs:element ref="xacml:Condition" minOccurs="0"></xs:element>
                        </xs:sequence>
                        <xs:attribute name="RuleId" type="xs:anyURI" use="required"></xs:attribute>
                        <xs:attribute name="Effect" type="xacml:EffectType" use="required"></xs:attribute>
                    </xs:complexType>
                    
                    <xs:simpleType name="EffectType">
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Permit"></xs:enumeration>
                            <xs:enumeration value="Deny"></xs:enumeration>
                        </xs:restriction>
                    </xs:simpleType>
                    
                    <xs:element name="Target" type="xacml:TargetType"></xs:element>
                    <xs:complexType name="TargetType">
                        <xs:sequence>
                            <xs:element ref="xacml:Subjects"></xs:element>
                            <xs:element ref="xacml:Resources"></xs:element>
                            <xs:element ref="xacml:Actions"></xs:element>
                        </xs:sequence>
                    </xs:complexType>
                    
                    <xs:element name="Subjects" type="xacml:SubjectsType"></xs:element>
                    <xs:complexType name="SubjectsType">
                        <xs:choice>
                            <xs:element ref="xacml:Subject" maxOccurs="unbounded"></xs:element>
                            <xs:element ref="xacml:AnySubject"></xs:element>
